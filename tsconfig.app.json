{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "src/types/env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.json"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "lib": ["es2017", "es2020", "dom"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
